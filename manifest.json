{
  "name": "Linkthereum",
  "description": "Converts ethereum addresses to links and provides additional infomation",
  "version": "1.0",
  "background": {
    "scripts": [
      "dist/hot-reload.js",
      "dist/worker.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "dist/content.js"
      ],
      "css": [
        "dist/content.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "dist/OpenSans-Regular.ttf"
  ],
  "permissions": [
    "storage"
  ],
  "manifest_version": 2
}
